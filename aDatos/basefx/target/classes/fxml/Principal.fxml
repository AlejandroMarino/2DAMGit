<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane fx:id="root" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="700"
            prefWidth="1000" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="ui.pantallas.principal.PrincipalController">
    <top>
        <MenuBar BorderPane.alignment="CENTER">
            <menus>
                <Menu fx:id="goTo" mnemonicParsing="false" text="Go to">
                    <items>
                        <MenuItem fx:id="itemLogout" mnemonicParsing="false" onAction="#goLogin" text="Logout"/>
                    </items>
                </Menu>
                <Menu fx:id="items" mnemonicParsing="false" text="items">
                    <items>
                        <MenuItem fx:id="itemNews" mnemonicParsing="false" onAction="#goNewspaper" text="newspapers"/>
                        <MenuItem fx:id="itemArti" mnemonicParsing="false" onAction="#goArti" text="articles"/>
                    </items>
                </Menu>
            </menus>
        </MenuBar>
    </top>
</BorderPane>
